// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appliers {
  userId Int      @id
  name   String
  role   String
  iat    Int
  exp    Int
  iss    String
  sub    Int
  movie  Movies[]
}

model Movies {
  id       String     @id @default(uuid())
  title    String
  released String
  genre    String
  director String
  applier  Appliers[]
}
